<div class="container">
	<h1>All Entries</h1>
	<% for entry in @entries %>
	 	<div class="border">
		   <h2><%= entry.title %></h2> [ <%= link_to("Edit this entry", edit_entry_path(entry)) %> ]
		   <p class="italic"><%= entry.date %></p>
		   <p><%= entry.contents %></p>
	  </div>
	<% end %>

	<%= link_to("New entry", new_entry_path) %> |
	<%= link_to("Back to home page", root_path) %>
</div>